VARIABLE MSB
VARIABLE LSB
VARIABLE INAR

: delay_1ms
   d# 8334 begin d# 1 - dup d# 0 = until drop	\ 100MHz
\ d# 4167 begin d# 1 - dup d# 0 = until	\ 50MHz
;

: delay_nms
   d# 0 do delay_1ms loop
;

: delay_10us
   d# 40 begin d# 1 - dup d# 0 = until drop	\ 100MHz
;

: delay_n_10us
   d# 0 do delay_10us loop
;

: suma
   ram_accum @ d# 1 + ram_accum !
;

: concatenar
   MSB dup @ swap drop
   d# 8 lshift MSB !
   MSB dup @ gp_out1 ! drop 
   LSB dup @ swap drop
   or INAR ! drop
   INAR dup @ gp_out1 ! drop    
;

: main 
   d# 242 MSB ! \ 1111 0010
\   MSB dup @ gp_out1 ! drop
   d# 163 LSB ! \ 1010 0011
\   LSB dup @ gp_out1 ! drop
   concatenar
;
